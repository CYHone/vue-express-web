#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vue-express-web/node_modules/.pnpm/sass@1.74.1/node_modules/sass/node_modules:/mnt/d/vue-express-web/node_modules/.pnpm/sass@1.74.1/node_modules:/mnt/d/vue-express-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/vue-express-web/node_modules/.pnpm/sass@1.74.1/node_modules/sass/node_modules:/mnt/d/vue-express-web/node_modules/.pnpm/sass@1.74.1/node_modules:/mnt/d/vue-express-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.74.1/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.74.1/node_modules/sass/sass.js" "$@"
fi
